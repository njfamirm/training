{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "lib": ["ES2020"],
    "strict": false,
    "incremental": true,
    "resolveJsonModule": true,
    "noEmitOnError": true,
    "tsBuildInfoFile": ".tsbuildinfo"
  }
}
